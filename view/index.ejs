	<script>
		let arrayN = <%- JSON.stringify(dataArray)%>;
		// console.log(arrayN);
		function Verification(){
			let villeDepart = document.getElementById("inputCityD").value;
			let villeArrive = document.getElementById("inputCityA").value;
			arrayN.forEach(element => {
				if(element.pointarrive == villeArrive){
					document.getElementById("disponibilité").innerHTML +=`
						<div class="row">
							<article class="card fl-left">
								<section class="date">
									<time>
										<span>${element.prix}</span>
										<p>escale : ${element.escale}</p>
									</time>
								</section>
								<section class="card-cont">
									<small>Safi Air</small>
									<div class="even-date">
									<i class="fa fa-calendar"></i>
									<input type="hidden" id="idvols" data-type="${element.volID}" value="${element.volID}">
									<time>
									<span class="ml-3">depart à ${element.datedepart} arrivée à ${element.datedarrive} </span>
									<span>Heure de depart à ${element.horaire}</span>
									<input type="hidden" id="capacite" value="${element.capacite}" >
									</time>
									</div>
									<div class="even-info">
									<i class="fa fa-map-marker"></i>
									<p>
										ville de depart ${element.pointdepart} ville d'arrivée ${element.pointarrive}
									</p>
									</div>
									<button class="btn btn-primary btn-md" onclick="reserver()" id="ticket" >tickets</button>
								</section> 
							</article>
						</div>
					`;
				}
			});
		}
		function reserver(){
			let idvols= document.getElementById('idvols').value;
			let capacite = document.getElementById("capacite").value;
			// console.log(capacite);
			document.getElementById('disponibilité').innerHTML=`
			<div class="tm-bg-black mb-5 ie-container-width-fix-2 mt-5">
			<h1>Reservation </h1>
			<div class="container ie-h-align-center-fix">
				<div class="row">
					<form action="/insertdata" method="post">
					<div class="col-xs-12 ml-auto mr-auto ie-container-width-fix">
						<div class="tm-search-form tm-section-pad-2">
							<p class="tm-margin-b-0" id="ncapacite"></p>
							<div class="form-row tm-search-form-row"> 
								<input type="hidden" id="capacite" value="${capacite}" >
								<div class="form-group tm-form-element tm-form-element-2">                                            
									<input class="form-control tm-select" type="number" max=20 min=1 id="nombrePer" onchange="myFunction()" name="nombrePer">
									
									<i class="fa fa-2x fa-user tm-form-element-icon"></i>
								</div>
								
								<div class="form-group tm-form-element tm-form-element-2">                                            
									<input type="text" class="form-control" id="nom" name="nom" placeholder="nom et prenom">
									<i class="fa fa-2x fa-user tm-form-element-icon"></i>
								</div>
								<input type="hidden" name="volID" value="${idvols}" >
								<div class="form-group tm-form-element tm-form-element-2">
									<input  type="text" class="form-control" id="email" name="email" placeholder="email">
									<i class="fa fa-2x fa-envelope tm-form-element-icon"></i>
								</div> 
								<div class="form-group tm-form-element tm-form-element-2">                                            
									<input  type="text" class="form-control" id="numeroTel"  name="numeroTel" placeholder="numero de telephone">
									<i class="fa fa-2x fa-phone tm-form-element-icon"></i>
								</div>
								<div class="form-group tm-form-element tm-form-element-2"> 	                                         
									<input  type="date" class="form-control" id="numeroTel" name="dateNaissance" placeholder="date de naissance">
									<i class="fa fa-2x fa-calendar tm-form-element-icon"></i>
								</div>
								<div class="form-group tm-form-element tm-form-element-2">
									<input  type="text" class="form-control" id="Npassport" name="passport" placeholder="numero de passport">
									<i class="fa fa-2x fa-globe tm-form-element-icon"></i>
								</div>
								<div class="form-group tm-form-element tm-form-element-2 ">  
									<div class="form-control d-flex">
										Assurance
										<input  type="checkbox" name="assurance" class="form-control mt-1">
									</div>
								</div> 
								<div class="form-group tm-form-element tm-form-element-2 ">  
									<div class="form-control d-flex">
										repas
										<input type="checkbox" name="repas" class="form-control mt-1">
									</div>
								</div>
								<div class="form-group tm-form-element tm-form-element-2">
									<button type="submit" class="btn btn-primary tm-btn-search" id="reserver" >Reserver maintenant</button>
